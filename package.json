{
    "name": "phaser-editor-template-vite-ts",
    "description": "A Phaser 3 TypeScript template using Vite.",
    "version": "1.2.1",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-vite-ts.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-vite-ts/issues"
    },
    "homepage": "https://github.com/phaserjs/template-vite-ts#readme",
    "scripts": {
        "start": "node start-servers.js",
        "dev-frontend": "vite --config vite/config.dev.mjs",
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs && phaser-asset-pack-hashing -j -r dist",
        "test": "npm run test:engine && npm run test:combat",
        "sync:labrute": "node scripts/sync-labrute-data.mjs",
        "compare:labrute": "node scripts/compare-labrute-data.mjs",
        "test:engine": "node --test server/combat/LaBruteEngine.test.mjs src/engine/formulas.test.mjs src/game/pets.test.mjs",
        "test:combat": "node scripts/runCombatTest.mjs --pets",
        "test:combat:snap": "node scripts/runCombatTest.mjs --snapshot --seed=42",
        "test:combat:debug": "node scripts/runCombatTest.mjs --debug --seed=42",
        "test:combat:debug:pets": "node scripts/runCombatTest.mjs --debug --pets --petA=panther --petB=dog --seed=42 --snapshot --output=.snapshots/fights/seed-42-debug-pets.json",
        "test:combat:exact": "node scripts/runCombatTest.mjs --formulas=exact",
        "test:combat:exact:debug": "node scripts/runCombatTest.mjs --formulas=exact --debug --seed=42",
        "export:meta": "tsx scripts/export-meta.ts",
        "build:sheets": "tsx scripts/build-sheets.ts"
    },
    "devDependencies": {
        "fast-csv": "^5.0.5",
        "phaser-asset-pack-hashing": "^1.0.6",
        "sharp": "^0.34.3",
        "spritesmith": "^0.21.2",
        "terser": "^5.28.1",
        "tsx": "^4.20.5",
        "typescript": "^5.3.3",
        "vite": "^7.1.3"
    },
    "dependencies": {
        "@esotericsoftware/spine-phaser": "^4.1.55",
        "@phaserjs/editor-scripts-base": "^1.0.0",
        "cors": "^2.8.5",
        "phaser": "3.80.1",
        "rand-seed": "^3.0.0",
        "socket.io": "^4.8.1"
    }
}
